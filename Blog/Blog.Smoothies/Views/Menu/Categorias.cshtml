@model  Blog.ViewModels.Menu.MenuCategoriasViewModel

@{
    Layout = null;
}
    
    <ul class="dropdown-menu">
        @foreach (var categoria in Model.Categorias)
        {
            <li>
                <a href="@Url.Action("Categoria", "Blog", new {id = categoria.UrlSlug})">@categoria.Nombre</a>
            </li>
        }

    </ul>


