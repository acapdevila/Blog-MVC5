@using Blog.ViewModels.Sidebar
@using Blog.Web.Helpers
@model ArchivoEtiquetasViewModel

@{
    Layout = null;
}

<div class="well">
    <h4>Archivo</h4>
    <div class="row">
        <div class="col-lg-12">
            <ul class="list-unstyled">
             @foreach (ArchivoItemViewModel item in Model.ListaArchivo)
             {
                 <li>
                     <a href="@Url.RutaUrlArchivo(anyo: item.Anyo, mes: item.Mes)">@item.NombreMes @item.Anyo (@item.NumPosts)</a>
                 </li>
             }
            </ul>
        </div>
      
    </div>
    <!-- /.row -->
</div>