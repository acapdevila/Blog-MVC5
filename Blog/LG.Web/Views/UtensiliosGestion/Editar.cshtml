@model LG.Web.Views.UtensiliosGestion.ViewModels.EditarUtensilioViewModel

@{
    ViewBag.Title = "Editar utensilio";
}



@Scripts.Render("~/bundles/controlesedicion")
@Styles.Render("~/bundles/controlesedicioncss")

<h2>Editar utensilio</h2>

@using (Html.BeginForm("Editar", "UtensiliosGestion", FormMethod.Post, new { id = "formulario", enctype = "multipart/form-data" }))
{
        @Html.AntiForgeryToken()

        <div class="form-horizontal">
            <h4>Editar utensilio</h4>
            <hr />
            @Html.ValidationSummary(true, "", new { @class = "text-danger" })


            @Html.EditorFor(m => m.Editor, "EditorDeUtensilio")

        </div>
    }


    @section Scripts {
        @Scripts.Render("~/bundles/jqueryval")
    }
